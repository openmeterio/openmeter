+        if (statusCode === "default") {
+            decorators.push({ name: "defaultResponse", args: [] });
+        }
+        else {
+            commonProperties.push(convertStatusCodeToProperty(statusCode));
+        }
+        if (isErrorStatusCode(statusCode)) {
+            decorators.push({ name: "error", args: [] });
+        }
