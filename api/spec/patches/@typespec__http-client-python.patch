+            return None
+        if self.code_model.options["models-mode"] == "msrest":
+            return self._get_attr_name(wire_name)
+        return wire_name
+
+    @property
+    def next_link_is_nested(self) -> bool:
+        return self.yaml_data.get("nextLinkIsNested", False)
+
+    @property
+    def item_name(self) -> str:
